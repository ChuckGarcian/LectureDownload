{
  "manifest_version": 3,
  "name": "Test Captions loader",
  "version": "1.0",
  "description": "scrapes captions and emails my personal email; this is to test automatic emailing api",
  "permissions": [
    "activeTab",
    "scripting",
    "webRequest",
    "tabs",
    "downloads"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "My Extension"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content.js"]
    }
  ],
  "background": {
    "service_worker": "dist/background.bundle.js"
  },
  "icons": {
    "16": "images/16x16.png",
    "48": "images/48x48.png",
    "128": "images/extension_128x128.png"
  }
}
